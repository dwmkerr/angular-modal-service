    "karma-phantomjs-launcher": "^0.2.1",
    "opn": "^3.0.1",
    "phantomjs": "^1.9.18",
    "tiny-lr": "^0.1.5",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
